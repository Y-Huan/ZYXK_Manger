\"仓库\" style=\"width: 150px;\">\n        <el-option\n          v-for=\"item in whse_list\"\n          :key=\"item.value\"\n          :label=\"item.whseName\"\n          :value=\"item.value\">\n        </el-option>\n      </el-select>\n      <el-input v-model.trim=\"reviewCreator\" clearable placeholder=\"经办人\" style=\"width: 150px; height: 50px;\"></el-input>\n      <el-date-picker\n        v-model=\"creat_time\"\n        type=\"daterange\"\n        range-separator=\"至\"\n        value-format=\"yyyy-MM-dd HH:mm:ss\"\n        start-placeholder=\"开始日期\"\n        style=\"width: 250px;\"\n        end-placeholder=\"结束日期\">\n      </el-date-picker>\n\n    </div>\n    <div class=\"search_box\">\n        <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getList(1)\">搜索</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-refresh-right\" @click=\"restParams\">重置</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-download\" @click=\"oper_detail_excel\">Excel导出入库单详情列表</el-button>\n    </div>\n    <div class=\"table_box\">\n      <el-table\n          :data=\"reviewOrderDetailList\"\n          stripe\n          border\n\t\t  class=\"table\"\n          height=\"100%\"\n          style=\"width: 100%\">\n          <el-table-column\n            align=\"center\"\n            prop=\"reviewOrderId\"\n            label=\"单号\"\n            width=\"190\">\n          </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"whseName\"\n          label=\"盘点仓库\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"productName\"\n          label=\"物品名称\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"skuId\"\n          label=\"物品编号\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"money\"\n          label=\"库存单价\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"账面数量\"\n          width=\"190\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.servicePiecegrods*scope.row.serviceWeight}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"盘点数量\"\n          width=\"190\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.piecegrods*scope.row.weight}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"surplusAmount\"\n          label=\"盈亏数量\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"surplusMoney\"\n          label=\"盈亏金额\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"creator\"\n          label=\"经办人\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          sortable\n          prop=\"createTime\"\n          label=\"入库时间\"\n          width=\"190\">\n        </el-table-column>\n\n        </el-table>\n    </div>\n  </div>\n</template>\n\n<script>\n  import {getReviewOrderDetaliList,whseList} from '@/api/revieworder'\n  import { parseTime } from '@/utils'\n  export default{\n    data(){\n      return{\n        whse_list:[],\n        reviewOrderId:'',\n        whse_id:'',\n        reviewCreator:'',\n        creat_time:[parseTime(new Date(new Date().getTime()- 1000*60*60*24*7),'{y}-{m}-{d} {h}:{i}:{s}'),parseTime(new Date(),'{y}-{m}-{d} {h}:{i}:{s}')],\n        reviewOrderDetailList:[],\n        total:0,\n        current:1,\n        editIndex:'',\n        show_dialog:false,\n        company_form:{\n          companyName:'',\n          companyAddress:''\n        }\n      }\n    },\n    mounted() {\n      this.getList();\n    },\n    methods:{\n      getList(page){\n        var data = {\n          pageNo:page||this.current,\n          pageSize:30,\n          reviewOrderId:this.reviewOrderId,\n          whseId:this.whse_id,\n          reviewCreator:this.reviewCreator\n        }\n        if(this.creat_time&&this.creat_time.length){\n          data.createTime =  this.creat_time[0];\n          data.endTime =  this.creat_time[1];\n        }\n        getReviewOrderDetaliList(data).then(res=>{\n          console.log(res);\n          this.reviewOrderDetailList = res;\n          this.total = res.total;\n          this.current = res.current;\n        })\n\n        whseList().then(res=>{\n          console.log(res)\n          this.whse_list = res;\n          this.total = res.total;\n          this.current = res.current;\n        })\n      },\n\n      restParams(){\n        this.reviewOrderId = '';\n        this.whse_id = '';\n        this.reviewCreator = '';\n        this.creat_time=[parseTime(new Date(new Date().getTime()- 1000*60*60*24*7),'{y}-{m}-{d} {h}:{i}:{s}'),parseTime(new Date(),'{y}-{m}-{d} {h}:{i}:{s}')];\n      },\n      oper_detail_excel(){\n        var data = {\n          reviewOrderId:this.reviewOrderId,\n          orderType:1,\n          whseId:this.whse_id,\n          reviewCreator:this.reviewCreator,\n          createTime:'',\n          endTime:''\n        }\n        if(this.creat_time&&this.creat_time.length){\n          data.createTime =  this.creat_time[0];\n          data.endTime =  this.creat_time[1];\n        }\n        window.open(process.env.VUE_APP_BASE_API+'/review/out/reviewDetailExcel?orderType='+2+'&operOrderId='+data.reviewOrderId+\n          '&whseId='+data.whseId+'&createTime='+data.createTime+'&endTime='+data.endTime+'&creator='+data.selectStringKey+'&token='+this.$store.state.user.token)\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .search_box{\n    .el-input,.el-select,.el-date-editor{\n      margin-right: 4px;\n    }\n  }\n  .table_box{\n    flex:1;\n    margin-top: 10px;\n    overflow: hidden;\n  }\n</style>\n"],"sourceRoot":"src/views/revieworder"}]}