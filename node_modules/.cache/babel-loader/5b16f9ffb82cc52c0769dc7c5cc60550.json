y}}\n      </el-descriptions-item>\n      <!---->\n    </el-descriptions>\n    <div class=\"search_box\">\n      <el-input v-model.trim=\"product\" clearable placeholder=\"物品名称/编号\" style=\"width: 150px; height: 50px;\"></el-input>\n      <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getProduct()\">搜索</el-button>\n    </div>\n    <div class=\"table_box\">\n      <el-table\n          :data=\"show_products\"\n          stripe\n          border\n\t\t  class=\"table\"\n          height=\"100%\"\n          style=\"width: 100%\">\n\n        <el-table-column\n          align=\"center\"\n          prop=\"productName\"\n          label=\"物品名称\"\n          width=\"250\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"skuId\"\n          label=\"物品编号\"\n          width=\"250\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"money\"\n          label=\"库存单价\"\n          width=\"250\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"账面数量\"\n          width=\"250\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.servicePiecegrods||scope.row.serviceWeight}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"盘点数量\"\n          width=\"250\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.piecegrods||scope.row.weight}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"盈亏数量\"\n          width=\"250\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.surplusAmount||0}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"盈亏金额\"\n          width=\"250\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.surplusMoney||0}}</span>\n          </template>\n        </el-table-column>\n        </el-table>\n    </div>\n\n\n    <div class=\"page_box\" v-if=\"total>30\">\n      <el-pagination\n        background\n        layout=\"prev, pager, next\"\n        @current-change=\"getInfo\"\n        :page-size=\"30\"\n        :current-page.sync=\"current\"\n        :total=\"total\">\n      </el-pagination>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\n  import {getReviewOrderByOperationOrderId} from '@/api/revieworder'\n  export default{\n    data(){\n      return{\n        whse_list:[{name:'启用',value:true},{name:'禁用',value:false}],\n        reviewOrderId:'',\n        whseName:'',\n        creator:'',\n        creat_time:'',\n        remark:'',\n        handleTime:'',\n        amount:'',\n        weight:'',\n        money:'',\n        show_products:[],\n        products:[],\n        pictures:[],\n        product:'',\n        total:0,\n        current:1,\n        editIndex:'',\n        show_dialog:false,\n        company_form:{\n          companyName:'',\n          companyAddress:''\n        }\n      }\n    },\n    mounted() {\n      this.getInfo();\n    },\n    methods:{\n      getInfo(){\n        var data = {\n          reviewOrderId:this.$route.params.id\n        }\n        getReviewOrderByOperationOrderId(data).then(res=>{\n          this.products = res.products;\n          this.show_products = res.products;\n          this.reviewOrderId = res.reviewOrderId;\n          this.whseName = res.whseName;\n          this.creator = res.creator;\n          this.remark = res.remark;\n          this.handleTime = res.createTime;\n          this.pictures = res.pictures;\n          this.amount = res.amount;\n          this.weight = res.weight;\n          this.money = res.money;\n        })\n      },\n      getProduct(){\n        this.show_products = this.products.filter(el=>{\n          return el.productName.indexOf(this.product)!=-1 || el.skuId.indexOf(this.product)!=-1\n        })\n\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .search_box{\n    .el-input,.el-select,.el-date-editor{\n      margin-right: 4px;\n    }\n  }\n  .table_box{\n    flex:1;\n    margin-top: 10px;\n    overflow: hidden;\n  }\n</style>\n"],"sourceRoot":"src/views/revieworder"}]}