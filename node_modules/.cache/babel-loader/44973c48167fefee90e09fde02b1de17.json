UAAA,GAAA;AACA,UAAA,WAAA,EAAA,EADA;AAEA,UAAA,IAAA,EAAA,EAFA;AAGA,UAAA,KAAA,EAAA,EAHA;AAIA,UAAA,IAAA,EAAA,EAJA;AAKA,UAAA,OAAA,EAAA;AALA,SAAA;AAOA,QAAA,MAAA,CAAA,WAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OAXA;AAaA;AA9EA;AArBA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search_box\">\n      <el-input v-model.trim=\"supplier_name\" clearable placeholder=\"供应商名称/手机号\" style=\"width: 200px;\"></el-input>\n      <el-date-picker\n        v-model=\"creat_time\"\n        type=\"daterange\"\n        range-separator=\"至\"\n        value-format=\"yyyy-MM-dd HH:mm:ss\"\n        start-placeholder=\"开始日期\"\n        style=\"width: 250px;\"\n        end-placeholder=\"结束日期\"\n        :default-time=\"['00:00:00', '23:59:59']\">\n      </el-date-picker>\n      <el-button-group>\n        <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getList(1)\">搜索</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-refresh-right\" @click=\"restParams\">重置</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-download\" @click=\"excel_out_payment\">导出应付表</el-button>\n      </el-button-group>\n    </div>\n    <div class=\"table_box\">\n      <el-table\n          :data=\"orderList\"\n          stripe\n          border\n          class=\"table\"\n          height=\"100%\"\n          style=\"width: 100%\">\n          <el-table-column label=\"操作\" width=\"250\" align=\"center\">\n            <template slot-scope=\"scope\">\n              <el-button\n                size=\"mini\"\n                type=\"primary\"\n                @click=\"paymentInfo(scope.$index,scope.row.supplierId,scope.row.supplierName)\">详情</el-button>\n              <el-button\n                size=\"mini\"\n                type=\"primary\"\n                @click=\"open_add_payment(scope.$index,scope.row.supplierId,scope.row.supplierName)\">添加付款记录</el-button>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"supplierName\"\n            label=\"供应商名称\">\n          </el-table-column>\n          <el-table-column\n            prop=\"openingBalance\"\n            label=\"期初余额\"\n            align=\"center\"\n            width=\"200\">\n          </el-table-column>\n          <el-table-column\n            prop=\"meet\"\n            label=\"期间产生额\"\n            align=\"center\"\n            width=\"200\">\n          </el-table-column>\n          <el-table-column\n            prop=\"paid\"\n            label=\"期间付款额\"\n            align=\"center\"\n            width=\"200\">\n          </el-table-column>\n          <el-table-column\n            prop=\"closingBalance\"\n            label=\"期末余额\"\n            align=\"center\"\n            width=\"200\">\n          </el-table-column>\n        </el-table>\n    </div>\n    <!-- 新增公司 -->\n    <el-dialog title=\"添加付款记录\" :visible.sync=\"show_dialog\" width=\"550px\">\n      <el-form :model=\"order_form\">\n        <el-form-item label=\"收款方\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"order_form.name\" disabled maxlength=\"30\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"付款金额\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"order_form.money\" clearable placeholder=\"请输入付款金额\" maxlength=\"30\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"付款时间\" label-width=\"120px\" required>\n          <el-date-picker\n            v-model=\"order_form.date\"\n            type=\"date\"\n            placeholder=\"选择日期\"\n            value-format=\"yyyy-MM-dd HH:mm:ss\">\n          </el-date-picker>\n        </el-form-item>\n        <el-form-item label=\"备注\" label-width=\"120px\">\n          <el-input v-model.trim=\"order_form.comment\" clearable placeholder=\"请输入备注\" maxlength=\"50\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"show_dialog = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"add_payment\">确 定</el-button>\n      </div>\n    </el-dialog>\n    <div class=\"page_box\" v-if=\"total>30\">\n      <el-pagination\n        background\n        layout=\"prev, pager, next\"\n        @current-change=\"getList\"\n        :page-size=\"30\"\n        :current-page.sync=\"current\"\n        :total=\"total\">\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {getPaymentList,addPayment} from '@/api/financial'\nimport { parseTime } from '@/utils'\n  export default{\n    data(){\n      return{\n        supplier_name:'',\n        creat_time:[parseTime(new Date(new Date().getTime()- 1000*60*60*24*30),'{y}-{m}-{d} ')+'00:00:00',parseTime(new Date(),'{y}-{m}-{d} ')+'23:59:59'],\n        orderList:[],\n        total:0,\n        current:1,\n        show_dialog:false,\n        order_form:{\n          supplier_id:'',\n          name:'',\n          money:'',\n          date:'',\n          comment:''\n        }\n      }\n    },\n    mounted() {\n      this.getList();\n    },\n    methods:{\n      getList(page){\n        var data = {\n          supplierName:this.supplier_name,\n\n        }\n        if(this.creat_time&&this.creat_time.length){\n          data.createTime =  this.creat_time[0];\n          data.endTime =  this.creat_time[1];\n        }\n        console.log(data);\n        getPaymentList(data).then(res=>{\n          this.orderList = res;\n        })\n      },\n      restParams(){\n        this.supplier_name = '';\n        this. creat_time=[parseTime(new Date(new Date().getTime()- 1000*60*60*24*30),'{y}-{m}-{d} {h}:{i}:{s}'),parseTime(new Date(),'{y}-{m}-{d} {h}:{i}:{s}')];\n      },\n      open_add_payment(index,id,name){\n\n        this.order_form = {\n          supplier_id:id,\n          name:name,\n          money:'',\n          date:'',\n          comment:''\n        }\n        this.show_dialog = true;\n      },\n      paymentInfo(index,id,name){\n        console.log(index);\n        this.$router.push('/financial/detail/'+id+'/'+name);\n      },\n      excel_out_payment(){\n        var data = {\n          supplierName:'',\n        }\n        if(this.customer_name){\n          data.supplierName = this.customer_name;\n        }\n        if(this.creat_time&&this.creat_time.length){\n          data.createTime =  this.creat_time[0];\n          data.endTime =  this.creat_time[1];\n        }\n\n        window.open(process.env.VUE_APP_BASE_API+\"/payment/excelOutPayment?supplierName=\"+data.supplierName+\"&createTime=\"+data.createTime+\"&endTime=\"+data.endTime+\n                                                                                                       \"&token=\"+this.$store.state.user.token)\n      },\n      add_payment(){\n        var data = {\n          supplierId:this.order_form.supplier_id,\n          paymentAmount:this.order_form.money,\n          paymentTime:this.order_form.date,\n          remark:this.order_form.comment,\n        }\n\n        if(!data.paymentTime){\n          this.$message.error('请选择付款时间');\n          return\n        }if(!data.paymentAmount){\n          this.$message.error('请输入付款金额');\n          return\n        }\n        console.log(data)\n        addPayment(data).then(res=>{\n          this.$message.success('添加成功');\n          this.order_form = {\n            supplier_id:'',\n            name:'',\n            money:'',\n            date:'',\n            comment:''\n          }\n          this.show_dialog = false;\n          this.getList();\n        })\n\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .search_box{\n    .el-input,.el-select,.el-date-editor{\n      margin-right: 4px;\n    }\n  }\n  .table_box{\n    flex:1;\n    margin-top: 10px;\n    overflow: hidden;\n  }\n</style>\n"],"sourceRoot":"src/views/financial"}]}