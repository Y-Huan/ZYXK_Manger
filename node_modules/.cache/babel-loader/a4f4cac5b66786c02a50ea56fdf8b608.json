lass=\"app-container\">\n    <div class=\"search_box\">\n      <el-input v-model.trim=\"product_name\" clearable placeholder=\"物品名称\" style=\"width: 200px;\"></el-input>\n      <el-input v-model.trim=\"sku_id\" clearable placeholder=\"物品编码\" style=\"width: 200px;\"></el-input>\n      <el-select v-model=\"whse_id\" placeholder=\"仓库\" style=\"width: 150px;\">\n        <el-option\n          v-for=\"item in whse_list\"\n          :key=\"item.whseId\"\n          :label=\"item.whseName\"\n          :value=\"item.whseId\">\n        </el-option>\n      </el-select>\n      <el-date-picker\n        v-model=\"creat_time\"\n        type=\"daterange\"\n        range-separator=\"至\"\n        value-format=\"yyyy-MM-dd HH:mm:ss\"\n        start-placeholder=\"开始日期\"\n        style=\"width: 250px;\"\n        end-placeholder=\"结束日期\"\n        :default-time=\"['00:00:00', '23:59:59']\">\n      </el-date-picker>\n    </div>\n    <div style=\"margin-top:20px\">\n      <el-button-group>\n        <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getList(1)\">搜索</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-refresh-right\" @click=\"restParams\">重置</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-download\" @click=\"out_product_excel\">导出物品领用统计列表</el-button>\n      </el-button-group>\n    </div>\n    <div class=\"table_box\">\n      <el-table :data=\"customerList\" stripe border class=\"table\" height=\"100%\" style=\"width: 100%\">\n        <el-table-column label=\"操作\" width=\"160\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <el-button size=\"mini\" type=\"primary\" @click=\"getDetail(scope.$index, scope.row.productId, scope.row.productName)\">详情</el-button>\n          </template>\n        </el-table-column>\n\n        <el-table-column prop=\"productName\" label=\"物品名称\" align=\"center\"></el-table-column>\n        <el-table-column prop=\"skuId\" label=\"物品编号\"  align=\"center\"></el-table-column>\n        <el-table-column  label=\"计量单位\"  align=\"center\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.unitType|sexToString}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"amount\" label=\"期间领用数量\" align=\"center\"></el-table-column>\n        <el-table-column prop=\"money\" label=\"期间领用金额\" align=\"center\"></el-table-column>\n      </el-table>\n    </div>\n    <!-- 物品领用详情 -->\n    <el-dialog title=\"物品领用详情\" :visible.sync=\"show_dialog\" width=\"650px\">\n\n      <span  style=\"font-size:15px\">物品名称:{{productName}}</span>\n\n      <el-table :data=\"productDetail\">\n        <el-table-column property=\"contact\" label=\"领用人名称\" width=\"200\"></el-table-column>\n        <el-table-column property=\"amount\" label=\"领用数量\" width=\"200\"></el-table-column>\n        <el-table-column property=\"money\" label=\"领用金额\"></el-table-column>\n      </el-table>\n    </el-dialog>\n    <div class=\"page_box\">\n      <el-pagination\n        background\n        layout=\"prev, pager, next\"\n        @current-change=\"getList\"\n        :page-size=\"15\"\n        :current-page.sync=\"current\"\n        :total=\"total\">\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {getRecordProductList,whseList,getProductDetail} from '@/api/record';\nimport { parseTime } from '@/utils'\nexport default {\n  data() {\n    return {\n      status_list: [{ name: '启用', value: true }, { name: '禁用', value: false }],\n      creat_time:[parseTime(new Date(new Date().getTime()- 1000*60*60*24*30),'{y}-{m}-{d} ')+'00:00:00',parseTime(new Date(),'{y}-{m}-{d} ')+'23:59:59'],\n      product_name:'',\n      sku_id:'',\n      whse_list:[],\n      whse_id:'',\n      customer_name: '',\n      customer_phone:'',\n      productName:'',\n      isEnable: '',\n      region: [],\n      city:[],\n      total:0,\n      current:1,\n      customerList: [],\n      productDetail:[],\n      regionName:'',\n      editIndex: '',\n      show_dialog: false,\n      customer_form: {\n        status:'',\n        name: '',\n        user:'',\n        amount: '',\n        city:'',\n        address:''\n      },\n      options:[],\n      props:{\n        value:'code',\n        label:'regionName',\n        children:'sonRegionList',\n        checkStrictly:true\n      }\n    };\n  },\n  mounted() {\n    this.getList();\n  },\n  filters: {\n    sexToString(val) {\n      if (val == 2) return 'KG';\n      if (val == 1) return '件';\n    }\n  },\n  methods: {\n    getList(page){\n      var data = {\n        pageNo:page||this.current,\n        pageSize:15,\n        productName:this.product_name,\n        skuId:this.sku_id,\n        whseId:this.whse_id,\n      }\n      if(this.creat_time&&this.creat_time.length){\n        data.createTime =  this.creat_time[0];\n        data.endTime =  this.creat_time[1];\n      }\n      console.log(data);\n      getRecordProductList(data).then(res=>{\n        this.customerList = res.records;\n        this.total = res.total;\n        this.current = res.current;\n      })\n      whseList().then(res=>{\n        this.whse_list = res;\n        this.total = res.total;\n        this.current = res.current;\n      })\n      },\n\n    restParams() {\n      this.product_name = ''\n      this.sku_id = '';\n      this.whse_id = '';\n      this.creat_time=[parseTime(new Date(new Date().getTime()- 1000*60*60*24*30),'{y}-{m}-{d} {h}:{i}:{s}'),parseTime(new Date(),'{y}-{m}-{d} {h}:{i}:{s}')];\n    },\n    open_add_customer() {\n      this.editIndex = '';\n      this.customer_form = {\n        status:'',\n        name: '',\n        user:'',\n        phone: '',\n        city:'',\n        address:''\n      };\n      this.show_dialog = true;\n    },\n    getDetail(index, id,productName) {\n      this.productName = productName;\n      var data = {\n        productId:id,\n        productName:this.product_name,\n        skuId:this.sku_id,\n        whseId:this.whse_id,\n      }\n      if(this.creat_time&&this.creat_time.length){\n        data.createTime =  this.creat_time[0];\n        data.endTime =  this.creat_time[1];\n      }\n      console.log(data)\n      getProductDetail(data).then(res=>{\n        this.productDetail = res;\n\n        this.show_dialog = true;\n      })\n    },\n    handleDelete(index, id) {\n      console.log(id)\n      this.$confirm('此操作将永久删除该供应商, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        delSupplier({companyId:id}).then(res=>{\n          this.$message.success('删除成功');\n          this.customerList.splice(index,1);\n        })\n      });\n    },\n    switchCompany(e, index, id) {\n      console.log({companyId:id})\n       enableCompany({companyId:id}).then(res=>{\n          this.$message.success('操作成功');\n          this.customerList[index].isEnable = e;\n\n        })\n    },\n    out_product_excel(){\n      var data = {\n        productName:this.product_name,\n        skuId:this.sku_id,\n        whseId:this.whse_id,\n      }\n      if(this.creat_time&&this.creat_time.length){\n        data.createTime =  this.creat_time[0];\n        data.endTime =  this.creat_time[1];\n      }\n      window.open(process.env.VUE_APP_BASE_API+\"/product/excelOutRecord?productName=\"+data.productName+\"&skuId=\"+data.skuId+\"&whseId=\"+data.whseId+'&createTime='+data.createTime+'&endTime='+data.endTime\n                                                                                              + \"&token=\"+this.$store.state.user.token)\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.search_box {\n  .el-input,\n  .el-select,\n  .el-cascader,\n  .el-date-editor {\n    margin-right: 4px;\n  }\n}\n.table_box {\n  flex: 1;\n  margin-top: 10px;\n  overflow: hidden;\n}\n</style>\n"],"sourceRoot":"src/views/record"}]}