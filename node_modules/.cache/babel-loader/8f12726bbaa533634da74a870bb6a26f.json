AA,KAAA,cAAA;;AACA,UAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA;;AACA,UAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA;;AACA,UAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA;AACA;;AACA,UAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,UAAA,EAAA,IAAA,CAAA,MADA;AAEA,QAAA,QAAA,EAAA,IAAA,CAAA,IAFA;AAGA,QAAA,UAAA,EAAA,IAAA,CAAA,GAHA;AAIA,QAAA,YAAA,EAAA,IAAA,CAAA,KAJA;AAKA,QAAA,SAAA,EAAA,IAAA,CAAA,GAAA,GAAA,IAAA,CAAA,GAAA,CAAA,EAAA,GAAA;AALA,OAAA;;AAOA,UAAA,KAAA,WAAA,IAAA,KAAA,WAAA,CAAA,MAAA,EAAA;AACA,YAAA,GAAA,GAAA,KAAA,KAAA,CAAA,WAAA,CAAA,cAAA,EAAA;AACA,QAAA,GAAA,GAAA,GAAA,CAAA,MAAA,CAAA,UAAA,EAAA,EAAA;AACA,iBAAA,EAAA,CAAA,OAAA,CAAA,KAAA,KAAA,CAAA,CAAA;AACA,SAFA,CAAA;;AAGA,YAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,eAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,GAAA;AACA;;AACA,UAAA,KAAA,WAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,KAAA,WAAA;AACA,QAAA,IAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA;;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA,SAJA;AAKA,OAPA,MAOA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,KAAA,EAAA;AACA,QAAA,GAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA;;AACA,UAAA,MAAA,CAAA,EAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA,SALA;AAMA;AACA,KAnGA;AAoGA,IAAA,UApGA,sBAoGA,KApGA,EAoGA,EApGA,EAoGA;AAAA;;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,CAAA,KAAA,CAAA;AACA,WAAA,WAAA,GAAA,IAAA,CAAA,MAAA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,GAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA,IAAA,CAAA,QAFA;AAGA,QAAA,GAAA,EAAA,IAAA,CAAA,UAHA;AAIA,QAAA,KAAA,EAAA,IAAA,CAAA,YAAA,IAAA,EAJA;AAKA,QAAA,MAAA,EAAA,IAAA,CAAA;AALA,OAAA;;AAOA,UAAA,IAAA,CAAA,UAAA,EAAA;AACA,QAAA,IAAA,CAAA,GAAA,GAAA;AAAA,UAAA,EAAA,EAAA,IAAA,CAAA,SAAA;AAAA,UAAA,GAAA,EAAA,IAAA,CAAA;AAAA,SAAA;AACA;;AACA,WAAA,cAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,cAAA,CAAA,GAAA;AACA,OAFA;AAGA,WAAA,WAAA,GAAA,IAAA;AACA,KAtHA;AAuHA,IAAA,YAvHA,wBAuHA,EAvHA,EAuHA;AAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,GAAA,CAAA;AAAA,UAAA,MAAA,EAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA;AACA,SAHA;AAIA,OATA;AAUA;AAlIA;AAvCA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search_box\">\n      <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" @click=\"open_add_warehouse\">新增</el-button>\n    </div>\n    <div class=\"table_box\">\n      <el-table\n          class=\"table\"\n          :data=\"list\"\n          stripe\n          border>\n          <el-table-column\n            type=\"index\"\n            width=\"50\"\n            align=\"center\"\n            label=\"序号\">\n          </el-table-column>\n          <el-table-column\n            align=\"center\"\n            label=\"仓库封面\"\n            width=\"250\">\n             <template slot-scope=\"scope\">\n               <el-image :preview-src-list=\"[scope.row.pictureUrl]\" :src=\"scope.row.pictureUrl\" v-if=\"scope.row.pictureUrl\"></el-image>\n               <el-image :preview-src-list=\"[default_img]\" :src=\"default_img\" v-else></el-image>\n             </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"whseName\"\n            width=\"200\"\n            label=\"仓库名称\">\n          </el-table-column>\n          <el-table-column\n            prop=\"whseNumber\"\n            label=\"仓库编号\"\n            width=\"150\">\n          </el-table-column>\n          <el-table-column\n            label=\"仓库位置\"\n            prop=\"whseLocation\">\n          </el-table-column>\n          <el-table-column\n            prop=\"whseHeader\"\n            label=\"负责人\"\n            align=\"center\"\n            width=\"100\">\n          </el-table-column>\n          <el-table-column\n            prop=\"creator\"\n            label=\"创建人\"\n            align=\"center\"\n            width=\"100\">\n          </el-table-column>\n          <el-table-column\n            sortable\n            prop=\"createTime\"\n            align=\"center\"\n            label=\"创建日期\"\n            width=\"200\">\n          </el-table-column>\n          <el-table-column label=\"操作\" width=\"150\" align=\"center\">\n            <template slot-scope=\"scope\">\n              <el-button\n                size=\"mini\"\n                type=\"primary\"\n                @click=\"handleEdit(scope.$index,scope.row.whseId)\">编辑</el-button>\n              <el-button\n                size=\"mini\"\n                type=\"danger\"\n                @click=\"handleDelete(scope.row.whseId)\">删除</el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n    </div>\n    <!-- 新增/编辑仓库 -->\n    <el-dialog :title=\"warehouseId?'编辑仓库':'新增仓库'\" v-loading=\"loading\" element-loading-text=\"上传中\" :visible.sync=\"show_dialog\" width=\"550px\">\n      <el-form :model=\"warehouse_form\">\n        <el-form-item label=\"仓库封面\" label-width=\"120px\">\n          <el-upload\n            class=\"cover-uploader\"\n            :action=\"upload_url\"\n            :multiple=\"false\"\n            :headers=\"{'token':token}\"\n            :data=\"{'source':1,sourceId:id}\"\n            :show-file-list=\"false\"\n            accept=\"image/jpeg,image/jpg,image/png\"\n            :before-upload=\"limitSize\"\n            :on-success=\"cover_upload_success\">\n            <img v-if=\"warehouse_form.img\" :src=\"warehouse_form.img.url\" class=\"avatar avatar-uploader-icon\">\n            <i v-if=\"warehouse_form.img\" @click.stop=\"warehouse_form.img=''\" class=\"el-icon-error\"></i>\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n          </el-upload>\n        </el-form-item>\n        <el-form-item label=\"仓库名称\" label-width=\"120px\" required clearable>\n          <el-input v-model.trim=\"warehouse_form.name\" placeholder=\"请输入仓库名称\" maxlength=\"10\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"仓库编号\" label-width=\"120px\" required clearable>\n          <el-input v-model.trim=\"warehouse_form.num\" placeholder=\"请输入仓库编号\" maxlength=\"20\" style=\"width: 200px;\"></el-input>\n          <el-button type=\"primary\" style=\"margin-left: 4px;\" @click=\"autoNum\">自动生成</el-button>\n        </el-form-item>\n        <el-form-item label=\"仓库负责人\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"warehouse_form.header\" placeholder=\"请输入仓库负责人\" maxlength=\"50\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"仓库位置\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"warehouse_form.local\" placeholder=\"请输入仓库位置\" maxlength=\"50\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"可视人\" label-width=\"120px\" required v-if=\"people_list&&people_list.length\">\n          <el-tree\n            style=\"max-height: 400px;overflow: auto;\"\n            :data=\"people_list\"\n            show-checkbox\n            ref=\"people_tree\"\n            node-key=\"userId\"\n            :check-on-click-node=\"true\"\n            :props=\"{label:'name',children:'sonDepartmentIds'}\">\n          </el-tree>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"show_dialog = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"add_warehouse\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\n  function formatData(data){\n    data.map(el=>{\n      if(el.userJwtVos&&el.userJwtVos.length){\n        el.sonDepartmentIds = [...el.sonDepartmentIds,...el.userJwtVos];\n      }\n      if(!el.name){\n        el.userId = el.departmentId;\n        el.name = el.departmentName;\n      }\n      if(el.userJwtVos&&el.sonDepartmentIds.length){\n        el.sonDepartmentIds = formatData(el.sonDepartmentIds);\n      }\n    })\n    return data;\n  }\n  import {getList,add,edit,del,get_num,get_id,get_people,get_people_list} from '@/api/warehouse'\n  export default{\n    data(){\n      return{\n        list:[],\n        loading:false,\n        show_dialog:false,\n        warehouseId:'',\n        id:'',\n        people_list:[],\n        checked_tree:[],\n        warehouse_form:{\n          img:'',\n          name:'',\n          num:'',\n          local:'',\n          header:''\n        }\n      }\n    },\n    computed:{\n      userInfo(){\n        return this.$store.state.user.userInfo;\n      },\n      default_img(){\n        return process.env.VUE_APP_BASE_OSS+'static/warehouse_default.png';\n      },\n      upload_url(){\n        return process.env.VUE_APP_BASE_API+'/picture/uploadImg'\n      },\n      token(){\n        return this.$store.state.user.token;\n      }\n    },\n    mounted() {\n      this.getList();\n      get_people_list().then(res=>{\n        this.people_list = formatData(res);\n      })\n    },\n    methods:{\n      limitSize(e){\n        if(e.size>1024*1024*2){\n          this.$message.error('上传图片大于2M');\n          return false\n        }else{\n          this.loading = true;\n        }\n      },\n      cover_upload_success(res){\n        if(res.code==200){\n          this.warehouse_form.img = res.data;\n          this.loading = false;\n        }\n      },\n      autoNum(){\n        get_num().then(res=>{\n          this.warehouse_form.num = res;\n        })\n      },\n      getList(){\n        getList().then(res=>{\n          this.list = res;\n        })\n      },\n      open_add_warehouse(){\n        this.warehouse_form = {\n          img:'',\n          name:'',\n          num:'',\n          local:'',\n          header:''\n        }\n        this.warehouseId = '';\n        if(this.id){\n          this.show_dialog = true;\n          this.$refs.people_tree.setCheckedKeys([]);\n        }else{\n          get_id({tableName:'INV_WHSE'}).then(res=>{\n            this.id = res;\n            this.show_dialog = true;\n            this.$nextTick(()=>{\n               this.$refs.people_tree.setCheckedKeys([]);\n            })\n          })\n        }\n      },\n      add_warehouse(){\n        var data = this.warehouse_form;\n        if(!data.name){\n          this.$message.error('请输入仓库名称');\n          return\n        }\n        if(!data.num){\n          this.$message.error('请输入仓库编号');\n          return\n        }\n        if(!data.header){\n          this.$message.error('请输入仓库负责人');\n          return\n        }\n        if(!data.local){\n          this.$message.error('请输入仓库位置');\n          return\n        }\n        var data = {\n          whseHeader:data.header,\n          whseName:data.name,\n          whseNumber:data.num,\n          whseLocation:data.local,\n          pictureId:data.img?data.img.id:'',\n        }\n        if(this.people_list&&this.people_list.length){\n          var ids = this.$refs.people_tree.getCheckedKeys();\n          ids = ids.filter(el=>{\n            return el.indexOf('BUR')!=-1;\n          })\n          if(ids.length==0){\n            this.$message.error('请至少选择一个可视人');\n            return\n          }\n          data.userIds = ids;\n        }\n        if(this.warehouseId){\n          data.whseId = this.warehouseId;\n          edit(data).then(res=>{\n            this.$message.success('编辑成功');\n            this.getList();\n            this.show_dialog = false;\n          })\n        }else{\n          data.whseId = this.id;\n          add(data).then(res=>{\n            this.$message.success('新增成功');\n            this.getList();\n            this.id = '';\n            this.show_dialog = false;\n          })\n        }\n      },\n      handleEdit(index,id){\n        var data = this.list[index];\n        this.warehouseId = data.whseId;\n        var form = {\n          img:'',\n          name:data.whseName,\n          num:data.whseNumber,\n          local:data.whseLocation||'',\n          header:data.whseHeader\n        }\n        if(data.pictureUrl){\n          form.img={id:data.pictureId,url:data.pictureUrl}\n        }\n        this.warehouse_form = form;\n        get_people({whseId:data.whseId}).then(res=>{\n          this.$refs.people_tree.setCheckedKeys(res);\n        })\n        this.show_dialog = true;\n      },\n      handleDelete(id){\n        this.$confirm('此操作将永久删除该仓库, 是否继续?', '提示', {\n        \tconfirmButtonText: '确定',\n        \tcancelButtonText: '取消',\n        \ttype: 'warning'\n        }).then(() => {\n        \tdel({whseId:id}).then(res=>{\n        \t\tthis.$message.success('删除成功');\n        \t\tthis.getList();\n        \t})\n        })\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .search_box{\n    .el-input,.el-select,.el-date-editor{\n      margin-right: 4px;\n    }\n  }\n  .table_box{\n    flex:1;\n    margin-top: 10px;\n    overflow: hidden;\n    .el-image{\n      max-width: 200px;\n      max-height: 100px;\n      display: block;\n      margin:0 auto;\n    }\n  }\n  .cover-uploader{\n    width: 160px;\n    height: 100px;\n    border: 1px dashed #ccc;\n    border-radius: 10px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    position: relative;\n    .avatar{\n      width: 100%;\n      height: 100%;\n      object-fit: cover;\n    }\n    i{\n      font-size: 32px;\n      line-height: 80px;\n    }\n    .el-icon-error{\n      color: red;\n      font-size: 20px;\n      position: absolute;\n      top: -5px;\n      right: -5px;\n      z-index: 2;\n      line-height: 1;\n    }\n  }\n</style>\n"],"sourceRoot":"src/views/warehouse"}]}