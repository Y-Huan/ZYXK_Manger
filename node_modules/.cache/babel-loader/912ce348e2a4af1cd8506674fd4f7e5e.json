,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,UAAA,CAAA;AAAA,UAAA,SAAA,EAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,SAHA;AAIA,OATA;AAUA,KAzHA;AA0HA,IAAA,aA1HA,yBA0HA,CA1HA,EA0HA,KA1HA,EA0HA,EA1HA,EA0HA;AAAA;;AACA,MAAA,aAAA,CAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,KAAA,EAAA,QAAA,GAAA,CAAA;AACA,OAHA;AAIA;AA/HA;AA/BA,CAAA","sourcesContent":["<template>\n\n  <div class=\"app-container\">\n    <div class=\"search_box\">\n      <el-input v-model.trim=\"order_id\" clearable placeholder=\"单号\" style=\"width: 150px; height: 50px;\"></el-input>\n      <el-select v-model=\"whse_id\" placeholder=\"仓库\" style=\"width: 150px;\">\n        <el-option\n          v-for=\"item in whse_list\"\n          :key=\"item.whseId\"\n          :label=\"item.whseName\"\n          :value=\"item.whseId\">\n        </el-option>\n      </el-select>\n      <el-input v-model.trim=\"creator\" clearable placeholder=\"经办人\" style=\"width: 150px; height: 50px;\"></el-input>\n      <el-date-picker\n        v-model=\"creat_time\"\n        type=\"daterange\"\n        range-separator=\"至\"\n        value-format=\"yyyy-MM-dd HH:mm:ss\"\n        start-placeholder=\"开始日期\"\n        style=\"width: 250px;\"\n        end-placeholder=\"结束日期\"\n      >\n      </el-date-picker>\n\n    </div>\n    <div class=\"search_box\">\n      <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getList(1)\">搜索</el-button>\n      <el-button type=\"primary\" icon=\"el-icon-refresh-right\" @click=\"restParams\">重置</el-button>\n      <el-button type=\"primary\" icon=\"el-icon-download\" @click=\"out_order_listExcel\">Excel导出入库单列表</el-button>\n    </div>\n    <div class=\"table_box\">\n      <el-table\n\t\tclass=\"table\"\n        :data=\"inorderList\"\n        stripe\n        border\n        height=\"100%\"\n        style=\"width: 100%\">\n        <el-table-column label=\"操作\" width=\"148\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <el-button type=\"text\" @click=\"orderInfo($event,scope.$index,scope.row.operOrderId)\">详情</el-button>\n            <el-button type=\"text\" @click=\"revoke($event,scope.$index,scope.row.operOrderId)\">撤销</el-button>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"operOrderId\"\n          label=\"入库单号\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"入库类型\"\n          width=\"190\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.type|sexToString}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"supplier\"\n          label=\"来源\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"whseName\"\n          label=\"入库仓库\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"入库件数总计\"\n          width=\"190\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.piecegoods||0}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"入库重量总计\"\n          width=\"190\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.weight||0}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"totalPrice\"\n          label=\"入库金额总计\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"creator\"\n          label=\"经办人\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          sortable\n          prop=\"handleTime\"\n          label=\"入库时间\"\n          width=\"190\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          sortable\n          prop=\"createTime\"\n          label=\"操作日期\"\n          width=\"190\">\n        </el-table-column>\n\n      </el-table>\n    </div>\n\n  </div>\n</template>\n\n<script>\n  import {updateInOperationOrderDetail,inorderList,whseList} from '@/api/inorder'\n  import { parseTime } from '@/utils'\n  export default{\n    data(){\n      return{\n        creat_time:[parseTime(new Date(new Date().getTime()- 1000*60*60*24*30),'{y}-{m}-{d} ')+'00:00:00',parseTime(new Date(),'{y}-{m}-{d} ')+'23:59:59'],\n        type_list:[{name:'采购入库',value:1},{name:'出库',value:2}],\n        whse_list:[],\n        order_id:'',\n        whse_id:'',\n        creator:'',\n\n        inorderList:[],\n        total:0,\n        current:1,\n        editIndex:'',\n        show_dialog:false,\n        company_form:{\n          companyName:'',\n          companyAddress:''\n        }\n      }\n    },\n    mounted() {\n      this.getList();\n      console.log(this.creat_time)\n    },\n    filters: {\n      sexToString(val) {\n        if (val == 2) return '出库';\n        if (val == 1) return '采购入库';\n      }\n    },\n    methods:{\n      getList(page){\n        var data = {\n          pageNo:page||this.current,\n          pageSize:30,\n          operOrderId:this.order_id,\n          orderType:1,\n          whseId:this.whse_id,\n          selectStringKey:this.creator\n        }\n        if(this.creat_time&&this.creat_time.length){\n          data.createTime =  this.creat_time[0];\n          data.endTime =  this.creat_time[1];\n        }\n        console.log(data);\n        inorderList(data).then(res=>{\n          this.inorderList = res.operationOrderViews;\n          this.total = res.total;\n          this.current = res.current;\n        })\n        whseList().then(res=>{\n          this.whse_list = res;\n          this.total = res.total;\n          this.current = res.current;\n        })\n      },\n      revoke(e,index,id) {\n        var data = {\n          operOrderId:id,\n          orderType:1,\n        }\n\n        this.$confirm('此操作将永久撤销该入库单, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }).then(()=> {\n         updateInOperationOrderDetail(data).then(res => {\n           console.log(res);\n             this.$message({\n               type: 'success',\n               message: '删除成功!'\n             });\n             this.inorderList.splice(index,1);\n\n         })\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          });\n        });\n      },\n      orderInfo(e,index,id){\n        this.$router.push('/inorder/detail/'+id);\n      },\n      restParams(){\n        this.order_id = '';\n        this.whse_id = '';\n        this.creator = '';\n        this.creat_time=[parseTime(new Date(new Date().getTime()- 1000*60*60*24*7),'{y}-{m}-{d} {h}:{i}:{s}'),parseTime(new Date(),'{y}-{m}-{d} {h}:{i}:{s}')];\n      },\n      out_order_listExcel(){\n        var data = {\n          operOrderId:this.order_id,\n          orderType:1,\n          whseId:this.whse_id,\n          selectStringKey:this.creator,\n          createTime:'',\n          endTime:''\n        }\n        if(this.creat_time&&this.creat_time.length){\n          data.createTime =  this.creat_time[0];\n          data.endTime =  this.creat_time[1];\n        }\n       window.open(process.env.VUE_APP_BASE_API+'/operation/out/operExcel?orderType='+1+'&operOrderId='+data.operOrderId+\n                        '&whseId='+data.whseId+'&createTime='+data.createTime+'&endTime='+data.endTime+'&creator='+data.selectStringKey+'&token='+this.$store.state.user.token)\n      },\n      add_company(){\n        if(!this.company_form.companyName){\n          this.$message.error('请输入公司名称');\n          return\n        }\n        if(!this.company_form.companyId){\n          creatCompany(this.company_form).then(res=>{\n            this.$message.success('添加成功');\n            this.getList();\n            this.show_dialog = false;\n          })\n        }else{\n          editCompany(this.company_form).then(res=>{\n            this.$message.success('保存成功');\n            this.companyList[this.editIndex].companyName = this.company_form.companyName;\n            this.companyList[this.editIndex].companyAddress = this.company_form.companyAddress;\n            this.show_dialog = false;\n            this.$forceUpdate();\n          })\n        }\n      },\n      handleEdit(index,id){\n        this.editIndex = index;\n        getCompany({companyId:id}).then(res=>{\n          this.company_form = {\n            companyName:res.companyName,\n            companyAddress:res.companyAddress,\n            companyId:id\n          }\n          this.show_dialog = true;\n        })\n      },\n      handleDelete(index,id){\n        this.$confirm('此操作将永久删除该公司, 是否继续?', '提示', {\n        \tconfirmButtonText: '确定',\n        \tcancelButtonText: '取消',\n        \ttype: 'warning'\n        }).then(() => {\n        \tdelCompany({companyId:id}).then(res=>{\n        \t\tthis.$message.success('删除成功');\n        \t\tthis.companyList.splice(index,1);\n        \t})\n        })\n      },\n      switchCompany(e,index,id){\n        enableCompany({companyId:id}).then(res=>{\n          this.$message.success('操作成功');\n          this.companyList[index].isEnable = e;\n        })\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .search_box{\n    .el-input,.el-select,.el-date-editor{\n      margin-right: 4px;\n    }\n  }\n  .table_box{\n    flex:1;\n    margin-top: 10px;\n    overflow: hidden;\n  }\n</style>\n"],"sourceRoot":"src/views/inorder"}]}