el-image style=\"height: 100px;width: 150px;margin-right:10px\"\n                  v-for=\"item in pictures\"\n                  :key=\"item\"\n                  :src=\"item\"\n                  :preview-src-list=\"pictures\">\n        </el-image>\n      </el-descriptions-item>\n    </el-descriptions>\n    <el-descriptions title=\"入库明细\">\n      <el-descriptions-item label=\"入库总计\">{{totalAmount}}件{{totalQuantity}}KG  ￥{{money}}</el-descriptions-item>\n\n\n    </el-descriptions>\n    <div class=\"search_box\">\n      <el-input v-model.trim=\"product\" clearable placeholder=\"物品名称/编号\" style=\"width: 150px; height: 50px;\"></el-input>\n      <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getProduct()\">搜索</el-button>\n    </div>\n    <div class=\"table_box\">\n      <el-table\n\t\t  class=\"table\"\n          :data=\"showOperOrderPrdouct\"\n          stripe\n          border\n          height=\"100%\"\n          style=\"width: 100%\">\n        <el-table-column\n          align=\"center\"\n          prop=\"productName\"\n          label=\"物品名称\"\n          width=\"336\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"skuId\"\n          label=\"物品编号\"\n          width=\"333\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          prop=\"unitPrice\"\n          label=\"入库单价\"\n          width=\"333\">\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"入库数量\"\n          width=\"333\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.piecegrods||scope.row.weight}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n          align=\"center\"\n          label=\"入库金额\"\n          width=\"333\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.weight*scope.row.unitPrice||scope.row.piecegrods*scope.row.unitPrice}}</span>\n          </template>\n        </el-table-column>\n        </el-table>\n\n    </div>\n\n\n    <div class=\"page_box\" v-if=\"total>30\">\n      <el-pagination\n        background\n        layout=\"prev, pager, next\"\n        @current-change=\"getInfo\"\n        :page-size=\"30\"\n        :current-page.sync=\"current\"\n        :total=\"total\">\n      </el-pagination>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\n  import {getOperationOrderByOperationOrderId} from '@/api/inorder'\n  export default{\n    data(){\n      return{\n        whse_list:[],\n        order_id:'',\n        whse_name:'',\n        creator:'',\n        creat_time:'',\n        remark:'',\n        handleTime:'',\n        supplierName:'',\n        orderType:'',\n        totalAmount:'',\n        totalQuantity:'',\n        supplier:'',\n        money:'',\n        operOrderProduct:[],\n        showOperOrderPrdouct:[],\n        pictures:[],\n        product:'',\n        total:0,\n        current:1,\n        editIndex:'',\n        show_dialog:false,\n        company_form:{\n          companyName:'',\n          companyAddress:''\n        }\n      }\n    },\n    mounted() {\n      this.getInfo();\n    },\n    filters: {\n      sexToString(val) {\n        if (val == 2) return '出库';\n        if (val == 1) return '采购入库';\n      }\n    },\n    methods:{\n      getInfo(){\n        var data = {\n          operOrderId:this.$route.params.id\n        }\n        getOperationOrderByOperationOrderId(data).then(res=>{\n          this.operOrderProduct = res.operOrderProduct;\n          this.showOperOrderPrdouct = res.operOrderProduct;\n          this.order_id = res.operOrderId;\n          this.whse_name = res.whseName;\n          this.creator = res.creator;\n          this.remark = res.remark;\n          this.orderType = res.orderType;\n          this.handleTime = res.handleTime;\n          this.pictures = res.pictures;\n          this.totalAmount = res.totalAmount;\n          this.totalQuantity = res.totalQuantity;\n          this.money = res.money;\n          this.supplier = res.supplier;\n          this.supplierName = res.supplierName;\n        })\n      },\n      getProduct(){\n        this.showOperOrderPrdouct = this.operOrderProduct.filter(el=>{\n          return el.productName.indexOf(this.product)!=-1 || el.skuId.indexOf(this.product)!=-1\n        })\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .search_box{\n    .el-input,.el-select,.el-date-editor{\n      margin-right: 4px;\n    }\n  }\n  .table_box{\n    flex:1;\n    margin-top: 10px;\n    overflow: hidden;\n  }\n</style>\n"],"sourceRoot":"src/views/inorder"}]}