" align=\"center\"></el-table-column>\n        <el-table-column prop=\"areaName\" label=\"所属区县\" width=\"150\" align=\"center\"></el-table-column>\n        <el-table-column prop=\"supplierAddress\" label=\"详细地址\" width=\"300\"></el-table-column>\n      </el-table>\n    </div>\n    <!-- 新增公司 -->\n    <el-dialog :title=\"editIndex?'编辑供应商':'新增供应商'\" :visible.sync=\"show_dialog\" width=\"650px\">\n      <el-form :model=\"customer_form\">\n        <el-form-item label=\"状态\" label-width=\"120px\" required>\n          <el-select v-model=\"customer_form.status\" placeholder=\"状态\" style=\"width: 100px;\">\n            <el-option v-for=\"item in status_list\" :key=\"item.value\" :label=\"item.name\" :value=\"item.value\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"供应商名称\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"customer_form.name\" clearable placeholder=\"请输入供应商名称\" maxlength=\"30\" style=\"width: 400px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"联系人\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"customer_form.user\" clearable placeholder=\"请输入联系人\" maxlength=\"30\" style=\"width: 400px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"联系电话\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"customer_form.phone\" clearable placeholder=\"请输入联系电话\" maxlength=\"30\" style=\"width: 400px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"所属地区\" label-width=\"120px\" required>\n          <el-cascader\n            v-model=\"city\"\n            placeholder=\"请选择省市区\"\n            :options=\"region\"\n            @change=\"changeCity\"\n            :props=\"props\"\n          >\n          </el-cascader>\n        </el-form-item>\n        <el-form-item label=\"详细地址\" label-width=\"120px\" required>\n          <el-input v-model.trim=\"customer_form.address\" clearable placeholder=\"请输入详细地址\" maxlength=\"50\" style=\"width: 400px;\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"show_dialog = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"add_customer\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport {getSupplierList,getRegion,addSupplier,enableCompany,editCompany,getCompany ,delSupplier} from '@/api/customer';\nexport default {\n  data() {\n    return {\n      status_list: [{ name: '启用', value: true }, { name: '禁用', value: false }],\n      customer_name: '',\n      customer_phone:'',\n      isEnable: '',\n      region: [],\n      city:[],\n      customerList: [],\n      regionName:'',\n      editIndex: '',\n      show_dialog: false,\n      customer_form: {\n        status:'',\n        name: '',\n        user:'',\n        phone: '',\n        city:'',\n        address:''\n      },\n      options:[],\n      props:{\n        value:'code',\n        label:'regionName',\n        children:'sonRegionList',\n        checkStrictly:true\n      }\n    };\n  },\n  mounted() {\n    this.getList();\n  },\n  methods: {\n    getList(page){\n      var data = {\n        pageNo:page||this.current,\n        pageSize:30,\n        supplierName:this.customer_name,\n        supplierPhone:this.customer_phone,\n      }\n      if(this.city.length==1){\n        data.province=this.city[0];\n      }\n      if(this.city.length==2){\n        data.province=this.city[0];\n        data.city=this.city[1];\n      }\n      if(this.city.length==3){\n        data.province=this.city[0];\n        data.city=this.city[1];\n        data.area=this.city[2];\n      }\n      console.log(data);\n      getSupplierList(data).then(res=>{\n        this.customerList = res.records;\n      })\n      getRegion().then(res=>{\n\n        this.region = res;\n        console.log(this.region);\n      })\n\n\n      },\n    changeCity(){\n      console.log(this.city)\n    },\n    restParams() {\n      this.customer_name = ''\n      this.customer_phone = '';\n      this.isEnable = '';\n      this.city = [];\n    },\n    open_add_customer() {\n      this.editIndex = '';\n      this.customer_form = {\n        status:'',\n        name: '',\n        user:'',\n        phone: '',\n        city:'',\n        address:''\n      };\n      this.show_dialog = true;\n    },\n    add_customer() {\n      var data = {\n        supplierId:this.customer_form.supplierId,\n        isEnable:this.customer_form.status,\n        supplierName:this.customer_form.name,\n        supplierPhone:this.customer_form.phone,\n        supplierAddress:this.customer_form.address,\n        supplierContact:this.customer_form.user,\n      }\n      if(this.city.length==1){\n        data.province=this.city[0];\n      }\n      if(this.city.length==2){\n        data.province=this.city[0];\n        data.city=this.city[1];\n      }\n      if(this.city.length==3){\n        data.province=this.city[0];\n        data.city=this.city[1];\n        data.area=this.city[2];\n      }\n      console.log(data);\n      if(!this.customer_form.name){\n        this.$message.error('请输入供应商名称');\n        return\n      } if(!this.customer_form.phone){\n        this.$message.error('请输入手机号');\n        return\n      } if(!this.customer_form.address){\n        this.$message.error('请输入详细地址');\n        return\n      } if(!this.customer_form.user){\n        this.$message.error('请输入联系人');\n        return\n      } if(this.city.length==0){\n        this.$message.error('请至少选择一个行政区');\n        return\n      }\n          addSupplier(data).then(res=>{\n            this.$message.success('更新成功');\n            this.city = [];\n            console.log(this.city);\n            this.$forceUpdate();\n            this.show_dialog = false;\n            this.getList();\n        })\n    },\n    handleEdit(index, id,isEnable) {\n      this.editIndex = id;\n      getCompany({supplierId:id}).then(res=>{\n        this.customer_form = {\n          name:res.supplierName,\n          phone:res.supplierPhone,\n          status:res.isEnable,\n          user:res.supplierContact,\n          address:res.supplierAddress,\n          supplierId:id,\n\n        }\n        this.city = [];\n        if(res.province){\n          this.city.push(res.province);\n        }\n        if(res.city) {\n          this.city.push(res.city);\n        }\n        if(res.area) {\n          this.city.push(res.area);\n        }\n        console.log(this.city);\n        this.show_dialog = true;\n      })\n    },\n    handleDelete(index, id) {\n      console.log(id)\n      this.$confirm('此操作将永久删除该供应商, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        delSupplier({companyId:id}).then(res=>{\n          this.$message.success('删除成功');\n          this.customerList.splice(index,1);\n        })\n      });\n    },\n    switchCompany(e, index, id) {\n      console.log({companyId:id})\n       enableCompany({companyId:id}).then(res=>{\n          this.$message.success('操作成功');\n          this.customerList[index].isEnable = e;\n\n        })\n    },\n    out_supplier_excel(){\n      var data = {\n        supplierName:this.customer_name,\n        supplierPhone:this.customer_phone,\n        province:'',\n        city:'',\n        area:'',\n\n\n      }\n      if(this.city.length==1){\n        data.province=this.city[0];\n      }\n      if(this.city.length==2){\n        data.province=this.city[0];\n        data.city=this.city[1];\n      }\n      if(this.city.length==3){\n        data.province=this.city[0];\n        data.city=this.city[1];\n        data.area=this.city[2];\n      }\n      window.open(process.env.VUE_APP_BASE_API+\"/supplier/excelOutSupplier?supplier=\"+data.supplierName+\"&supplierPhone=\"+data.supplierPhone+\"&province=\"+data.province+\"&city=\"+data.city+\n                                                                     \"&area=\"+data.area+ \"&token=\"+this.$store.state.user.token)\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.search_box {\n  .el-input,\n  .el-select,\n  .el-cascader,\n  .el-date-editor {\n    margin-right: 4px;\n  }\n}\n.table_box {\n  flex: 1;\n  margin-top: 10px;\n  overflow: hidden;\n}\n</style>\n"]}]}